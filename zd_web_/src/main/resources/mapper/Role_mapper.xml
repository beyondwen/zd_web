<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.cn.dao.Roledao">
<insert id="createRole" parameterType="com.cn.pojo.Role_pojo">
  insert into t_role(id,rolename,description,available) 
  values(
     #{#id},
     #{rolename},
     #{description},
     #{available}
  )
</insert>
</mapper>